@Test [EOL]     public void testPrimitivesToWrappers() { [EOL]         // test null [EOL] //        assertNull("null -> null", ClassUtils.primitivesToWrappers(null)); // generates warning [EOL]         assertNull("null -> null", ClassUtils.primitivesToWrappers((Class<?>[]) null)); // equivalent cast to avoid warning [EOL]         // Other possible casts for null [EOL]         assertTrue("empty -> empty", Arrays.equals(ArrayUtils.EMPTY_CLASS_ARRAY, ClassUtils.primitivesToWrappers())); [EOL]         final Class<?>[] castNull = ClassUtils.primitivesToWrappers((Class<?>)null); // == new Class<?>[]{null} [EOL]         assertTrue("(Class<?>)null -> [null]", Arrays.equals(new Class<?>[]{null}, castNull)); [EOL]         // test empty array is returned unchanged [EOL]         // TODO this is not documented [EOL]         assertArrayEquals("empty -> empty", [EOL]                 ArrayUtils.EMPTY_CLASS_ARRAY, ClassUtils.primitivesToWrappers(ArrayUtils.EMPTY_CLASS_ARRAY)); [EOL]  [EOL]         // test an array of various classes [EOL]         final Class<?>[] primitives = new Class[] { [EOL]                 Boolean.TYPE, Byte.TYPE, Character.TYPE, Short.TYPE, [EOL]                 Integer.TYPE, Long.TYPE, Double.TYPE, Float.TYPE, [EOL]                 String.class, ClassUtils.class [EOL]         }; [EOL]         final Class<?>[] wrappers= ClassUtils.primitivesToWrappers(primitives); [EOL]  [EOL]         for (int i=0; i < primitives.length; i++) { [EOL]             // test each returned wrapper [EOL]             final Class<?> primitive = primitives[i]; [EOL]             final Class<?> expectedWrapper = ClassUtils.primitiveToWrapper(primitive); [EOL]  [EOL]             assertEquals(primitive + " -> " + expectedWrapper, expectedWrapper, wrappers[i]); [EOL]         } [EOL]  [EOL]         // test an array of no primitive classes [EOL]         final Class<?>[] noPrimitives = new Class[] { [EOL]                 String.class, ClassUtils.class, Void.TYPE [EOL]         }; [EOL]         /